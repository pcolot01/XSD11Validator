	<xs:complexType name="t_numbering" mixed="true">
		<xs:openContent mode="interleave">
			<xs:any namespace="##other" processContents="lax"/>
		</xs:openContent>
		<xs:sequence/>
		<xs:assert test="matches(fn:normalize-space(.),'^(\d+(\.\d+)*\.?|\(\d+\)|\(?\w{1,2}\)|\([ivxlcdm]+\)|[IVXLCDM]+\.?|\d+\w(\.\d+\w)*\.?|ONE|TWO|THREE|FOUR|FIVE|SIX|SEVEN|EIGHT|NINE|TEN|ELEVEN|\w+\.?)$')">
			<xs:annotation>
				<xs:documentation>
					<cv:rule>
						<cv:cov_version>2.1</cv:cov_version>
						<cv:status>assertion</cv:status>
						<cv:title>Numbering format</cv:title>
						<cv:definition>Numbering has to respect previewed formats.</cv:definition>
						<cv:example>
							<cv:title>Common numbering</cv:title>
							<cv:code>"1", "1.", "5.2."<cv:br/>
matched by: '\d+(\.\d+)*\.?'</cv:code>
						</cv:example>
						<cv:example>
							<cv:title>Numbering in parentheses</cv:title>
							<cv:code>"(10)"<cv:br/>
matched by: '\(\d+\)'</cv:code>
						</cv:example>
						<cv:example>
							<cv:title>Letter numbering in parentheses</cv:title>
							<cv:code>"(a)", "a)", "(ca)"<cv:br/>
matched by: '\(?\w{1,2}\)'</cv:code>
						</cv:example>
						<cv:example>
							<cv:title>Greek numbering in parenthesis</cv:title>
							<cv:code>"(iii)"<cv:br/>
matched by: '\([ivxlcdm]+\)'</cv:code>
						</cv:example>
						<cv:example>
							<cv:title>Greek numbering</cv:title>
							<cv:code>"I", "I."<cv:br/>
matched by: '[IVXLCDM]+\.?'</cv:code>
						</cv:example>
						<cv:example>
							<cv:title>Numbering with a letter</cv:title>
							<cv:code>"3a.", "1a.2b"<cv:br/>
matches: '\d+\w(\.\d+\w)*\.?'</cv:code>
						</cv:example>
						<cv:example>
							<cv:title>Numbering with a word</cv:title>
							<cv:code>"ONE", "TWO"<cv:br/>
								matches: 'ONE|TWO'</cv:code>
						</cv:example>
						<cv:example>
							<cv:title>Numbering with a capital letter</cv:title>
							<cv:code>"A."<cv:br/>
								matches: '\w+\.?'</cv:code>
						</cv:example>
					</cv:rule>
				</xs:documentation>
			</xs:annotation>
		</xs:assert>
	</xs:complexType>
