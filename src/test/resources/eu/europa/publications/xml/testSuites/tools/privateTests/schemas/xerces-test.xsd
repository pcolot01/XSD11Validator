	<!--
	#############################################################################
	#
	# Administrative information
	#
	#############################################################################
	-->
	<xs:annotation>
		<xs:documentation>
			<cv:title>IMFC — Common Vocabulary</cv:title>
			<cv:version draft="true">2.1</cv:version>
			<cv:date>2019-06-29</cv:date>

			<cv:bibliography>
				<cv:title>Normative documentation</cv:title>
				<cv:bib_unit link="https://ec.europa.eu/info/sites/info/files/joint_handbook_en_lt.pdf">JH - Joint Handbook for the presentation and drafting of acts subject to the Ordinary Legislative Procedure</cv:bib_unit>
				<cv:bib_unit link="http://eur-lex.europa.eu/content/techleg/KB0213228ENN.pdf">JPG - Joint Practical Guide for persons involved in the drafting of European Union legislation</cv:bib_unit>
			</cv:bibliography>

			<cv:bibliography>
				<cv:title>Informative documentation</cv:title>
				<cv:bib_unit link="http://publications.europa.eu/code/en/en-000100.htm">ISG - Interinstitutional Style Guide</cv:bib_unit>
			</cv:bibliography>
		</xs:documentation>
	</xs:annotation>
	<!--
	####################################################################################################
	#
	# Type definitions
	#
	####################################################################################################
	-->
	<xs:complexType name="t_acting_entity" mixed="true">
		<xs:openContent mode="interleave">
			<xs:any namespace="##other" processContents="lax"/>
		</xs:openContent>
		<xs:sequence/>
	</xs:complexType>

	<xs:complexType name="t_annex" mixed="true">
		<xs:openContent mode="interleave">
			<xs:any namespace="##other" processContents="lax"/>
		</xs:openContent>
		<xs:choice>
			<!-- standard annex -->
			<xs:sequence>
				<xs:element ref="ifc:heading" minOccurs="0"/>
				<xs:element ref="ifc:subject" minOccurs="0"/>
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:element ref="ifc:title"/>
					<xs:element ref="ifc:part"/>
					<xs:element ref="ifc:chapter"/>
					<xs:element ref="ifc:section"/>
					<xs:element ref="ifc:table"/>
					<xs:element ref="ifc:unnumbered_paragraph"/>
					<xs:element ref="ifc:point"/>
					<xs:element ref="ifc:indent"/>
					<xs:element ref="ifc:introductory_part"/>
					<xs:element ref="ifc:closing_part"/>
					<xs:element ref="ifc:note"/>
					<xs:element ref="ifc:quotation">
						<xs:annotation>
							<xs:documentation>
								<cv:contains>
									<cv:allow_empty/>
								</cv:contains>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:choice>
			</xs:sequence>
		</xs:choice>
		<!-- TODO provide the test -->
		<xs:assert test="fn:true()">
			<xs:annotation>
				<xs:documentation>
					<cv:rule>
						<cv:cov_version>2.1</cv:cov_version>
						<cv:status>assertion</cv:status>
						<cv:title>A subject of an annex</cv:title>
						<cv:definition>If there is no subject in the annex and a single section with a subject so the subject has to be moved to the annex level.</cv:definition>
					</cv:rule>
				</xs:documentation>
			</xs:annotation>
		</xs:assert>
		<xs:assert test="not(.//ifc:*[local-name()=('unnumbered_paragraph','subparagraph') and (ifc:introductory_part|ifc:point|ifc:indent) and following-sibling::ifc:*[1][local-name()=('unnumbered_paragraph','subparagraph') and matches(fn:normalize-space(string-join(.//text(),'')),'^[a-z]')]])">
			<xs:annotation>
				<xs:documentation>
					<!-- this rule is applicable within: t_annex, t_enacting_parts -->
					<cv:rule>
						<cv:cov_version>2.1</cv:cov_version>
						<cv:status>assertion</cv:status>
						<cv:title>Avoid a closing part marked as an unnumbered paragraph or subparagraph</cv:title>
						<cv:definition>If there is a paragraph, containing introductory_part+points/indents, followed by another paragraph, containing text in the first node, and this text starts with a small letter, so it should be a closing_part.</cv:definition>
						<cv:example>
							<cv:title>Wrong</cv:title>
							<cv:code>&lt;unnumbered_paragraph&gt;<cv:br/>
<cv:indent/>&lt;introductory_part&gt;Following:&lt;/introductory_part&gt;<cv:br/>
<cv:indent/>&lt;point&gt;&lt;numbering&gt;(i)&lt;/numbering&gt; first point&lt;/point&gt;<cv:br/>
<cv:indent/>&lt;point&gt;&lt;numbering&gt;(ii)&lt;/numbering&gt; second point&lt;/point&gt;<cv:br/>
&lt;/unnumbered_paragraph&gt;<cv:br/>
&lt;unnumbered_paragraph&gt;are important points.&lt;/unnumbered_paragraph&gt;</cv:code>
						</cv:example>
						<cv:example>
							<cv:title>Correct</cv:title>
							<cv:code>&lt;unnumbered_paragraph&gt;<cv:br/>
<cv:indent/>&lt;introductory_part&gt;Following:&lt;/introductory_part&gt;<cv:br/>
<cv:indent/>&lt;point&gt;&lt;numbering&gt;(i)&lt;/numbering&gt; first point&lt;/point&gt;<cv:br/>
<cv:indent/>&lt;point&gt;&lt;numbering&gt;(ii)&lt;/numbering&gt; second point&lt;/point&gt;<cv:br/>
<cv:indent/>&lt;closing_part&gt;are important points.&lt;/closing_part&gt;<cv:br/>
&lt;/unnumbered_paragraph&gt;</cv:code>
						</cv:example>
					</cv:rule>
				</xs:documentation>
			</xs:annotation>
		</xs:assert>
		<!-- TODO delete: we decided to use subparagraphs
			TODO is there a rule that ensures the use of a subparagraph in the annex?
		<xs:assert test="not(.//ifc:point[not(ancestor::ifc:article)]/ifc:subparagraph)">
			<xs:annotation>
				<xs:documentation>
					Points inside of an annex, unless it is an annex with articles, use
					an unnumbered_paragraph and not the subparagraph as in the enacting
					terms points.
				</xs:documentation>
			</xs:annotation>
		</xs:assert> -->
	</xs:complexType>
	<xs:complexType name="t_annexes">
		<xs:openContent mode="interleave">
			<xs:any namespace="##other" processContents="lax"/>
		</xs:openContent>
		<xs:sequence>
			<xs:element ref="ifc:logo"/>
			<xs:element ref="ifc:place_and_date"/>
			<xs:element ref="ifc:reference"/>
			<xs:element ref="ifc:version" minOccurs="0"/>
			<xs:element ref="ifc:heading"/>
			<xs:element ref="ifc:subject"/>
			<xs:element ref="ifc:annex" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="t_article" mixed="true">
		<xs:openContent mode="interleave">
			<xs:any namespace="##other" processContents="lax"/>
		</xs:openContent>
		<xs:sequence>
			<xs:element ref="ifc:heading"/>
			<xs:element ref="ifc:subject" minOccurs="0"/>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="ifc:numbered_paragraph"/>
				<xs:element ref="ifc:unnumbered_paragraph"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="t_change_to_original_text" mixed="true">
		<xs:sequence/>
		<xs:attribute name="type" type="xs:string">
			<xs:annotation>
				<xs:documentation>refonte_deletion/adapted/addition/deletion</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="severity" type="xs:string">
			<xs:annotation>
				<xs:documentation>minor</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="t_chapter" mixed="true">
		<xs:sequence>
			<xs:element ref="ifc:heading"/>
			<xs:element ref="ifc:subject"/>
			<xs:element ref="ifc:article" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="ifc:section" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="ifc:point" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="ifc:indent" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>
						<cv:contains>
							<cv:allow_empty>XMLHD-1614</cv:allow_empty>
						</cv:contains>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="t_citation" mixed="true">
		<xs:openContent mode="interleave">
			<xs:any namespace="##other" processContents="lax"/>
		</xs:openContent>
		<xs:sequence>
			<xs:element ref="ifc:indent" minOccurs="0"/>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="ifc:note"/>
				<xs:element ref="ifc:legal_basis"/>
				<xs:element ref="ifc:date"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="t_closing_part" mixed="true">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<!-- In general, the content of the closing_part should be the same as for the introductory_part  -->
			<xs:element ref="ifc:date"/>
			<xs:element ref="ifc:note"/>
			<xs:element ref="ifc:quotation">
				<xs:annotation>
					<xs:documentation>
						<cv:contains>
							<cv:allow_empty/>
						</cv:contains>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="ifc:scientific_formula">
				<xs:annotation>
					<xs:documentation>
						<cv:contains>
							<cv:allow_empty/>
						</cv:contains>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
		<xs:assert test="matches(fn:normalize-space(text()), '^[a-z]')">
			<xs:annotation>
				<xs:documentation>
					<cv:rule>
						<cv:cov_version>2.1</cv:cov_version>
						<cv:status>assertion</cv:status>
						<cv:title>Closing part starts with a lower-case</cv:title>
						<cv:definition>The closing part has to start with a small letter because it is a continuation of a sentence started in the introductory part.</cv:definition>
					</cv:rule>
				</xs:documentation>
			</xs:annotation>
		</xs:assert>
	</xs:complexType>
	<xs:complexType name="t_Commission_communication" mixed="true">
		<xs:sequence>
			<xs:element ref="ifc:procedure_identifier"/>
			<xs:element ref="ifc:title_of_the_document" minOccurs="0"/>
			<xs:element ref="ifc:section" maxOccurs="unbounded"/>
			<xs:element ref="ifc:signature" minOccurs="0"/>
			<xs:element ref="ifc:annex" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="t_Commission_opinion" mixed="true">
		<xs:sequence>
			<xs:element ref="ifc:procedure_identifier"/>
			<xs:element ref="ifc:title_of_the_document" minOccurs="0"/>
			<xs:element ref="ifc:section" maxOccurs="unbounded"/>
			<xs:element ref="ifc:signature" minOccurs="0"/>
			<xs:element ref="ifc:annex" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="t_communication" mixed="true">
		<xs:sequence>
			<xs:element ref="ifc:logo"/>
			<xs:element ref="ifc:place_and_date"/>
			<xs:element ref="ifc:reference"/>
			<xs:element ref="ifc:version" minOccurs="0"/>
			<xs:element ref="ifc:procedure_identifier"/>
			<xs:element ref="ifc:title_of_the_document"/>
			<xs:choice>
				<xs:element ref="ifc:Commission_communication"/>
			</xs:choice>
		</xs:sequence>
		<xs:assert test="not(.//ifc:point[not(ancestor::ifc:article)]/ifc:subparagraph)">
			<xs:annotation>
				<xs:documentation>
					<cv:rule>
						<cv:cov_version>2.1</cv:cov_version>
						<cv:status>assertion</cv:status>
						<cv:title>Markup of paragraphs in a context of an annex</cv:title>
						<cv:definition>Points inside of an annex, unless it is an annex with articles, use an unnumbered paragraph and not the subparagraph as in the enacting terms points.</cv:definition>
					</cv:rule>
				</xs:documentation>
			</xs:annotation>
		</xs:assert>
	</xs:complexType>
	<xs:complexType name="t_decision">
		<xs:sequence>
			<xs:element ref="ifc:procedure_identifier" minOccurs="0"/>
			<!-- TODO: assertion Decision that is a part of the position does not need to have the title_of_the_document, otherwise it is mandatory -->
			<xs:element ref="ifc:title_of_the_document" minOccurs="0"/>
			<xs:element ref="ifc:EEA_relevance" minOccurs="0"/>
			<xs:element ref="ifc:preamble"/>
			<xs:element ref="ifc:enacting_terms"/>
			<xs:element ref="ifc:signature"/>
			<xs:element ref="ifc:annex" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="t_directive" mixed="true">
		<xs:sequence>
			<xs:element ref="ifc:procedure_identifier" minOccurs="0"/>
			<xs:choice>
				<xs:element ref="ifc:title_of_the_document"/>
			</xs:choice>
			<xs:element ref="ifc:EEA_relevance" minOccurs="0"/>
			<xs:element ref="ifc:preamble"/>
			<xs:element ref="ifc:enacting_terms"/>
			<xs:element ref="ifc:signature"/>
			<xs:element ref="ifc:annex" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="t_enacting_terms" mixed="true">
		<xs:openContent mode="interleave">
			<xs:any namespace="##other" processContents="lax"/>
		</xs:openContent>
		<xs:choice>
			<xs:element ref="ifc:article" minOccurs="1" maxOccurs="unbounded"/>
			<xs:element ref="ifc:chapter" minOccurs="1" maxOccurs="unbounded"/>
			<xs:element ref="ifc:numbered_paragraph" minOccurs="1" maxOccurs="unbounded"/>
			<xs:element ref="ifc:part" minOccurs="1" maxOccurs="unbounded"/>
			<xs:element ref="ifc:title" minOccurs="1" maxOccurs="unbounded"/>
		</xs:choice>
		<xs:assert test="not(.//ifc:point/ifc:unnumbered_paragraph)">
			<xs:annotation>
				<xs:documentation>
					<cv:rule>
						<cv:cov_version>2.1</cv:cov_version>
						<cv:status>assertion</cv:status>
						<cv:title>Markup of paragraphs in a context of the enacting terms</cv:title>
						<cv:definition>Points inside of the enacting terms use a subparagraph and not the unnumbered_paragraph as in the annex points.</cv:definition>
					</cv:rule>
				</xs:documentation>
			</xs:annotation>
		</xs:assert>
		<xs:assert test="not(.//ifc:*[local-name()=('unnumbered_paragraph','subparagraph') and (ifc:introductory_part|ifc:point|ifc:indent) and following-sibling::ifc:*[1][local-name()=('unnumbered_paragraph','subparagraph') and matches(fn:normalize-space(string-join(.//text(),'')),'^[a-z]')]])">
			<xs:annotation>
				<xs:documentation>
					<!-- this rule is applicable within: t_annex, t_enacting_parts -->
					<cv:rule>
						<cv:cov_version>2.1</cv:cov_version>
						<cv:status>assertion</cv:status>
						<cv:title>Avoid a closing part marked as an unnumbered paragraph or subparagraph</cv:title>
						<cv:definition>If there is a paragraph, containing introductory_part+points/indents, followed by another paragraph, containing text in the first node, and this text starts with a small letter, so it should be a closing_part.</cv:definition>
						<cv:example>
							<cv:title>Wrong</cv:title>
							<cv:code>&lt;unnumbered_paragraph&gt;
	&lt;introductory_part&gt;Following:&lt;/introductory_part&gt;
	&lt;point&gt;&lt;numbering&gt;(i)&lt;/numbering&gt; first point&lt;/point&gt;
	&lt;point&gt;&lt;numbering&gt;(ii)&lt;/numbering&gt; second point&lt;/point&gt;
&lt;/unnumbered_paragraph&gt;
&lt;unnumbered_paragraph&gt;are important points.&lt;/unnumbered_paragraph&gt;</cv:code>
						</cv:example>
						<cv:example>
							<cv:title>Correct</cv:title>
							<cv:code>&lt;unnumbered_paragraph&gt;
	&lt;introductory_part&gt;Following:&lt;/introductory_part&gt;
	&lt;point&gt;&lt;numbering&gt;(i)&lt;/numbering&gt; first point&lt;/point&gt;
	&lt;point&gt;&lt;numbering&gt;(ii)&lt;/numbering&gt; second point&lt;/point&gt;
	&lt;closing_part&gt;are important points.&lt;/closing_part&gt;
&lt;/unnumbered_paragraph&gt;</cv:code>
						</cv:example>
					</cv:rule>
				</xs:documentation>
			</xs:annotation>
		</xs:assert>
	</xs:complexType>
	<xs:complexType name="t_explanatory_memorandum" mixed="true">
		<xs:sequence>
			<xs:element ref="ifc:subject" minOccurs="0"/>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="ifc:section"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="t_heading" mixed="true">
		<xs:openContent mode="interleave">
			<xs:any namespace="##other" processContents="lax"/>
		</xs:openContent>
		<xs:sequence>
			<xs:element ref="ifc:numbering" minOccurs="0"/>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="ifc:note"/>
			</xs:choice>
		</xs:sequence>
		<xs:assert test="not(text()) or text()[matches(fn:normalize-space(.),'^(annex|chapter|section|title|part|ad article|—|article|objective|sub\s*-?\s*section|subject:?)$','i')]">
			<xs:annotation>
				<xs:documentation>
					<cv:rule>
						<cv:cov_version>2.1</cv:cov_version>
						<cv:status>assertion</cv:status>
						<cv:title>Typical heading patterns</cv:title>
						<cv:definition>If the heading specifies a text that does not match one of the specified strings so it should be rather a subject.</cv:definition>
						<cv:example>
							<cv:title>Avoid</cv:title>
							<cv:code>&lt;heading&gt;&lt;numbering&gt;1.&lt;/numbering&gt; General information&lt;/heading&gt;</cv:code>
						</cv:example>
						<cv:example>
							<cv:title>Use instead</cv:title>
							<cv:code>&lt;heading&gt;&lt;numbering&gt;1.&lt;/numbering&gt;&lt;/heading&gt; &lt;subject&gt;General information&lt;/subject&gt;</cv:code>
						</cv:example>
					</cv:rule>
				</xs:documentation>
			</xs:annotation>
		</xs:assert>
	</xs:complexType>
	<xs:complexType name="t_implementing_regulation">
		<xs:openContent mode="interleave">
			<xs:any namespace="##other" processContents="lax"/>
		</xs:openContent>
		<xs:sequence>
			<xs:element ref="ifc:title_of_the_document"/>
			<xs:element ref="ifc:EEA_relevance" minOccurs="0"/>
			<xs:element ref="ifc:preamble"/>
			<xs:element ref="ifc:enacting_terms"/>
			<xs:element ref="ifc:direct_applicability"/>
			<xs:element ref="ifc:signature"/>
			<xs:element ref="ifc:annex" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="t_indent" mixed="true">
		<xs:openContent mode="interleave">
			<xs:any namespace="##other" processContents="lax"/>
		</xs:openContent>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element ref="ifc:date"/>
			<xs:element ref="ifc:note"/>
			<xs:element ref="ifc:quotation">
				<xs:annotation>
					<xs:documentation>
						<cv:contains>
							<cv:allow_empty>XMLHD-1614</cv:allow_empty>
						</cv:contains>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="ifc:indent"/>
			<xs:element ref="ifc:introductory_part"/>
			<xs:element ref="ifc:closing_part"/>
			<xs:element ref="ifc:scientific_formula"/>
			<xs:element ref="ifc:subparagraph">
				<xs:annotation>
					<xs:documentation>
						<cv:contains>
							<cv:allow_empty>XMLHD-1614</cv:allow_empty>
						</cv:contains>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="ifc:table">
				<xs:annotation>
					<xs:documentation>
						<cv:contains>
							<cv:allow_empty>XMLHD-1614</cv:allow_empty>
						</cv:contains>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
		<xs:attribute name="class"/>
		<xs:assert test="text()[1][matches(fn:normalize-space(.),'^—')]">
			<xs:annotation>
				<xs:documentation>
					<cv:rule>
						<cv:cov_version>2.1</cv:cov_version>
						<cv:status>assertion</cv:status>
						<cv:title>Indent start character</cv:title>
						<cv:definition>An indent has to start with a dash '—'.</cv:definition>
					</cv:rule>
				</xs:documentation>
			</xs:annotation>
		</xs:assert>
	</xs:complexType>
	<xs:complexType name="t_international_agreement">
		<xs:sequence>
			<xs:element ref="ifc:procedure_identifier" minOccurs="0"/>
			<xs:element ref="ifc:title_of_the_document"/>
			<xs:element ref="ifc:EEA_relevance" minOccurs="0"/>
			<xs:element ref="ifc:preamble"/>
