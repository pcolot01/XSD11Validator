	<xs:complexType name="t_point" mixed="true">
		<xs:openContent mode="interleave">
			<xs:any namespace="##other" processContents="lax"/>
		</xs:openContent>
		<xs:sequence>
			<xs:element ref="ifc:numbering" minOccurs="0"/>
			<!--
			The idea is that a point will always have just <numbering/> instead of <heading><numbering/></heading>.
			In that case possibly we don't need the heading anymore. - TODO to be verified
			<xs:element ref="ifc:heading" minOccurs="0"/>
			-->
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="ifc:indent"/>
				<xs:element ref="ifc:subparagraph"/>
				<!--
				Do we now use always subparagraph under the point?
				<xs:element ref="ifc:unnumbered_paragraph"/>
				-->
				<xs:element ref="ifc:point"/>
				<xs:element ref="ifc:note"/>
				<xs:element ref="ifc:scientific_formula"/>
				<xs:element ref="ifc:date"/>
				<xs:element ref="ifc:table"/>
				<xs:element ref="ifc:text_of_the_amendment"/>
				<xs:element ref="ifc:introductory_part"/>
				<xs:element ref="ifc:closing_part"/>
				<xs:element ref="ifc:quotation"/>
				<xs:element ref="ifc:legal_basis"/>
			</xs:choice>
		</xs:sequence>
		<!-- TODO provide the test -->
		<xs:assert test="fn:true()">
			<xs:annotation>
				<xs:documentation>
					<cv:rule>
						<cv:cov_version>2.1</cv:cov_version>
						<cv:status>assertion</cv:status>
						<cv:title>No point subject in annexes</cv:title>
						<cv:definition>Subject is not supposed to be used in annexes points (use subparagraph instead).</cv:definition>
						<cv:comment>Currently it seems still to be used e.g. in statement_of_reasons points so it can't be removed completely.</cv:comment>
					</cv:rule>
				</xs:documentation>
			</xs:annotation>
		</xs:assert>
		<xs:assert test="not(ifc:heading) or ifc:heading[fn:normalize-space(text())]">
			<xs:annotation>
				<xs:documentation>
					<cv:rule>
						<cv:cov_version>2.1</cv:cov_version>
						<cv:status>assertion</cv:status>
						<cv:title>Sole numbering in a point</cv:title>
						<cv:definition>Use just "numbering" without "heading" if there is no extra text.</cv:definition>
						<cv:example>
							<cv:title>Avoid</cv:title>
							<cv:code>&lt;heading&gt;<cv:br/>
<cv:indent/>&lt;numbering&gt;1.&lt;/numbering&gt;<cv:br/>
&lt;/heading&gt;</cv:code>
						</cv:example>
						<cv:example>
							<cv:title>Use instead</cv:title>
							<cv:code>&lt;numbering&gt;1.&lt;/numbering&gt;</cv:code>
						</cv:example>
					</cv:rule>
				</xs:documentation>
			</xs:annotation>
		</xs:assert>
		<xs:assert test="not(ifc:unnumbered_paragraph and (ifc:introductory_part or ifc:point))">
			<xs:annotation>
				<xs:documentation>
					<cv:rule>
						<cv:cov_version>2.1</cv:cov_version>
						<cv:status>assertion</cv:status>
						<cv:title>Points in unnumbered paragraphs</cv:title>
						<cv:definition>If there are some "unnumbered_paragraph"s in the point so the "introductory_part" + "point"s has to be encapsulated in the unnumbered_paragraph as well.</cv:definition>
						<cv:example>
							<cv:title>Avoid</cv:title>
							<cv:code>&lt;unnumbered_paragraph&gt;text&lt;/unnumbered_paragraph&gt;<cv:br/>
&lt;introductory_part&gt;intro:&lt;/introductory_part&gt;<cv:br/>
&lt;point&gt;text&lt;/point&gt;</cv:code>
						</cv:example>
						<cv:example>
							<cv:title>Use instead</cv:title>
							<cv:code>&lt;unnumbered_paragraph&gt;text&lt;/unnumbered_paragraph&gt;<cv:br/>
&lt;unnumbered_paragraph&gt;<cv:br/>
<cv:indent/>&lt;introductory_part&gt;intro:&lt;/introductory_part&gt;<cv:br/>
<cv:indent/>&lt;point&gt;text&lt;/point&gt;<cv:br/>
&lt;/unnumbered_paragraph&gt;</cv:code>
						</cv:example>
					</cv:rule>
				</xs:documentation>
			</xs:annotation>
		</xs:assert>
		<xs:assert test="not(text()[fn:normalize-space(.)!='' and matches(fn:replace(.,'^\s*(.+)\s*$','$1'),'\n')])">
			<xs:annotation>
				<xs:documentation>
					<cv:rule>
						<cv:cov_version>2.1</cv:cov_version>
						<cv:status>assertion</cv:status>
						<cv:title>Missing paragraphs</cv:title>
						<cv:definition>If the '\n' is found so there is probably a missing &lt;unnumbered_paragraph/&gt; or &lt;subparagraph/&gt;.</cv:definition>
						<cv:comment>This rule helps to detect problems especially in files converted from Formex. In the other files it ensures common formatting.</cv:comment>
						<cv:example>
							<cv:title>Avoid</cv:title>
							<cv:code>Text.<cv:br/>
Text.</cv:code>
						</cv:example>
						<cv:example>
							<cv:title>Use instead</cv:title>
							<cv:code>Text. Text.</cv:code>
						</cv:example>
						<cv:example>
							<cv:title>Or</cv:title>
							<cv:code>&lt;unnumbered_paragraph&gt;Text.&lt;/unnumbered_paragraph&gt;<cv:br/>
&lt;unnumbered_paragraph&gt;Text.&lt;/unnumbered_paragraph&gt;</cv:code>
						</cv:example>
						<cv:example>
							<cv:title>Or</cv:title>
							<cv:code>&lt;subparagraph&gt;Text.&lt;/subparagraph&gt;<cv:br/>
&lt;subparagraph&gt;Text.&lt;/subparagraph&gt;</cv:code>
						</cv:example>
					</cv:rule>
				</xs:documentation>
			</xs:annotation>
		</xs:assert>
		<xs:assert test="not(count(ifc:unnumbered_paragraph|ifc:subparagraph|ifc:table)=1)">
			<xs:annotation>
				<xs:documentation>
					<cv:rule>
						<cv:cov_version>2.1</cv:cov_version>
						<cv:status>assertion</cv:status>
						<cv:title>No single paragraph in a point</cv:title>
						<cv:definition>Point does not allow a standalone &lt;unnumbered_paragraph/&gt; or a single &lt;subparagraph/&gt;. Their use is permitted only when there are more of them. However, a table is not encapsulated by a subparagraph.</cv:definition>
					</cv:rule>
				</xs:documentation>
			</xs:annotation>
		</xs:assert>
	</xs:complexType>
